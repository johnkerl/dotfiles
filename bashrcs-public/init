# vim: set filetype=sh:

# This is intended to be sourced from ../bashrc.
# See also ./README.md for context.

self=$(__get_source_name)
selfdir=$(dirname $self)

__maybe_say SELF IS $self

# Source color-rc first so other scripts can use colorized output.
for file in \
  color-rc \
  prompt-rc \
  path-rc \
  general-rc \
  git-rc \
  quarto-rc \
  python-rc \
  cpp-rc \
  rust-rc \
  docker-rc \
  r-rc \
  java-rc \
  miller-rc \
  lumin-rc
do
  . $selfdir/$file
done
